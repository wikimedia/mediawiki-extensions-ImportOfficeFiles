{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"parserOptions": {
		"sourceType": "commonjs",
		"ecmaVersion": 2020
	},
	"globals": {
		"officeimport": "readonly"
	},
	"rules": {
		"es-x/no-async-functions": "off",
		"es-x/no-optional-chaining": "off",
		"max-len": "off",
		"no-jquery/no-done-fail": "warn"
	}
}
